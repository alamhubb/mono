{
    "name": "test-workspace",
    "lockfileVersion": 3,
    "requires": true,
    "packages": {
        "": {
            "name": "test-workspace",
            "workspaces": [
                "packages/*"
            ]
        },
        "node_modules/@test/app": {
            "resolved": "packages/app",
            "link": true
        },
        "node_modules/@test/utils": {
            "resolved": "packages/utils",
            "link": true
        },
        "packages/app": {
            "name": "@test/app",
            "version": "1.0.0",
            "dependencies": {
                "@test/utils": "*"
            }
        },
        "packages/utils": {
            "name": "@test/utils",
            "version": "1.0.0"
        }
    }
}
