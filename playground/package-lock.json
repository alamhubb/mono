{
  "name": "playground",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "playground",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/@playground/app": {
      "resolved": "packages/app",
      "link": true
    },
    "node_modules/@playground/utils": {
      "resolved": "packages/utils",
      "link": true
    },
    "packages/app": {
      "name": "@playground/app",
      "version": "1.0.0",
      "dependencies": {
        "@playground/utils": "*"
      }
    },
    "packages/utils": {
      "name": "@playground/utils",
      "version": "1.0.0"
    }
  }
}
